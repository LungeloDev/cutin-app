{
  "expo": {
    "name": "cutin-app",
    "slug": "cutin-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/cutin-logo.png",
    "scheme": "cutinapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 2,
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/cutin-logo.png",
        "backgroundImage": "./assets/images/cutin-logo.png",
        "monochromeImage": "./assets/images/cutin-logo.png" 
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.lungelodev.cutinapp"
    },
    "web": {
      "output": "static",
      "bundler": "metro",
      "favicon": "./assets/images/cutin-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/cutin-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "53af0f63-de98-4a4e-9ed7-b169f821267a"
      }
    }
  }
}